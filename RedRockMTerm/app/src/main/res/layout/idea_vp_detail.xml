<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:scaleType="fitXY"
        android:id="@+id/ins_big_image"
        android:layout_width="190dp"
        android:layout_height="500dp"
        android:layout_marginBottom="268dp"
        android:src="@drawable/image_5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/ins_side_image_1"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/color_round"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.728"
        app:layout_constraintStart_toEndOf="@+id/ins_big_image"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ins_side_image_3"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:src="@drawable/color_round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.714"
        app:layout_constraintStart_toEndOf="@+id/ins_big_image"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <ImageView
        android:id="@+id/ins_side_image_2"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:src="@drawable/color_round"
        app:layout_constraintBottom_toTopOf="@+id/ins_side_image_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.727"
        app:layout_constraintStart_toEndOf="@+id/ins_big_image"
        app:layout_constraintTop_toBottomOf="@+id/ins_side_image_1"
        app:layout_constraintVertical_bias="0.534" />

    <TextView
        android:id="@+id/ins_bottom_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="色卡"
        android:textColor="@color/black"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/ins_bottom_constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ins_big_image"
        app:layout_constraintVertical_bias="0.505" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ins_bottom_constraintLayout"
        android:layout_width="353dp"
        android:layout_height="143dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ins_big_image"
        app:layout_constraintVertical_bias="0.546">


        <TextView
            android:id="@+id/ins_bottom_text_1"
            android:layout_width="100dp"
            android:layout_height="36dp"
            android:layout_marginStart="16dp"
            android:background="#1B5D3C"
            android:gravity="right|bottom"
            android:text="乙"
            android:textColor="@color/white"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.149" />

        <TextView
            android:id="@+id/ins_bottom_text_3_hex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="52dp"
            android:text="#1B5D3C"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ins_bottom_text_3" />

        <TextView
            android:id="@+id/ins_bottom_text_2"
            android:layout_width="100dp"
            android:layout_height="36dp"
            android:background="#1B5D3C"
            android:gravity="right|bottom"
            android:text="丙"
            android:textColor="@color/white"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/ins_bottom_text_3"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintStart_toEndOf="@+id/ins_bottom_text_1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.149" />

        <TextView
            android:id="@+id/ins_bottom_text_2_hex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#1B5D3C"
            app:layout_constraintEnd_toStartOf="@+id/ins_bottom_text_3_hex"
            app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintStart_toEndOf="@+id/ins_bottom_text_1_hex"
            app:layout_constraintTop_toBottomOf="@+id/ins_bottom_text_2" />

        <TextView
            android:id="@+id/ins_bottom_text_3"
            android:layout_width="100dp"
            android:layout_height="36dp"
            android:layout_marginEnd="12dp"
            android:background="#1B5D3C"
            android:gravity="right|bottom"
            android:text="丁"
            android:textColor="@color/white"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.149" />

        <TextView
            android:id="@+id/ins_bottom_text_4_hex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="#1B5D3C"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ins_bottom_text_4" />

        <TextView
            android:id="@+id/ins_bottom_text_4"
            android:layout_width="100dp"
            android:layout_height="36dp"
            android:layout_marginStart="16dp"
            android:background="#1B5D3C"
            android:gravity="right|bottom"
            android:text="戊"
            android:textColor="@color/white"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ins_bottom_text_1" />

        <TextView
            android:id="@+id/ins_bottom_text_5_hex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:text="#1B5D3C"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/ins_bottom_text_6_hex"
            app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintStart_toEndOf="@+id/ins_bottom_text_4_hex"
            app:layout_constraintTop_toBottomOf="@+id/ins_bottom_text_5"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/ins_bottom_text_5"
            android:layout_width="100dp"
            android:layout_height="36dp"
            android:background="#1B5D3C"
            android:gravity="right|bottom"
            android:text="己"
            android:textColor="@color/white"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/ins_bottom_text_6"
            app:layout_constraintStart_toEndOf="@+id/ins_bottom_text_4"
            app:layout_constraintTop_toBottomOf="@+id/ins_bottom_text_2" />

        <TextView
            android:id="@+id/ins_bottom_text_1_hex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="#1B5D3C"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ins_bottom_text_1" />

        <TextView
            android:id="@+id/ins_bottom_text_6"
            android:layout_width="100dp"
            android:layout_height="36dp"
            android:layout_marginEnd="12dp"
            android:background="#1B5D3C"
            android:gravity="right|bottom"
            android:text="庚"
            android:textColor="@color/white"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ins_bottom_text_3" />

        <TextView
            android:id="@+id/ins_bottom_text_6_hex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="52dp"
            android:text="#1B5D3C"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ins_bottom_text_6" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>